<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="acs_physician_public" model="ir.rule">
            <field name="name">Public Physician</field>
            <field name="model_id" ref="acs_hms_base.model_hms_physician"/>
            <field name="domain_force">[('website_published', '=', True)]</field>
            <field name="groups" eval="[
                Command.link(ref('base.group_public')),
                Command.link(ref('base.group_portal')),
            ]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_unlink" eval="False"/>
        </record>

    </data>
</odoo>
