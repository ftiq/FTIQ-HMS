<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="patient_information_template" inherit_id="acs_radiology.patient_information_template">
        <xpath expr="//tr[@name='radiology_test_date']" position="after">
            <tr name="raiology_app_null" t-if="radiology_data and radiology_data.appointment_id">
                <td name="raiology_appointment">
                    <strong class="text-center ms-2">Appointment:</strong> <span t-field="radiology_data.appointment_id"/>
                </td>
                <td name="radiology_test_null"></td>
            </tr>
        </xpath>
    </template>

</odoo>