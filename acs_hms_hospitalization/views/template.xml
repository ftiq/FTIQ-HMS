<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="acs_no_deathregister_details" name="Death Register Detail">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="oe_structure"/>
                <div class="container mt64">
                    <center>
                        <h2>No Related Details Available. Please check it again.</h2>
                    </center>
                </div>
            </div>
        </t>
    </template>
    <template id="acs_deathregister_details" name="Death Register Detail">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <div id="prescription_content" class="col-12 col-lg mt32 mb64 o_portal_html_view shadow p-3" style="background-color: white;">
                        <t t-call="acs_hms_hospitalization.report_hospital_death_register">
                            <t t-set="docs" t-value="[deathregister.sudo()]"/>
                        </t>
                    </div>
                </div>
            </div>
        </t>
    </template>
</odoo>
