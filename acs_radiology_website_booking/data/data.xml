<?xml version="1.0"?>
<odoo>
    <data noupdate="1">

        <record id="menu_acs_radiology_appointment" model="website.menu">
            <field name="name">Radiology Booking</field>
            <field name="url">/create/radiology/appointment</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">30</field>
        </record>

        <record forcecreate="True" id="ir_cron_clear_radiology_appointment" model="ir.cron">
            <field name="name">Clear Radiology Appointment</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="model_id" ref="acs_radiology.model_acs_radiology_request"/>
            <field name="state">code</field>
            <field name="code">model.clear_acs_radiology_request_cron()</field>
        </record>

     </data>
</odoo>