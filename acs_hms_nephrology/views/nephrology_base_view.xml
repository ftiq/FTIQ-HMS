<?xml version="1.0"?>
<odoo>

    <!-- Race -->
    <record id="acs_hms_race" model="ir.ui.view">
        <field name="name">Race</field>
        <field name="model">acs.race</field>
        <field name="arch" type="xml">
            <list string="Race">
                <field name="name"/>
                <field name="code"/>
            </list>
        </field>
    </record>

    <record id="acs_hms_race_form" model="ir.ui.view">
        <field name="name">Race</field>
        <field name="model">acs.race</field>
        <field name="arch" type="xml">
            <form string="Race">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="code"/>
                        </group>
                        <separator string="Description"/>
                        <field name="description" placeholder="Description" nolabel="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_acs_race" model="ir.actions.act_window">
        <field name="name">Race</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">acs.race</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Record Found
            </p>
            <p>
                Click to add new Race.
            </p>
        </field>
    </record>

